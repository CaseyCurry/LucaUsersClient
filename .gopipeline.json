        "run_if": "passed",
        "on_cancel": null,
        "command": "bash",
        "arguments": [
          "-c",
          "./node_modules/.bin/mocha --opts mocha.opts --reporter mocha-multi --reporter-options mochawesome=-,xunit=specs.xml"
        ],
        "working_directory": null
      }, {
        "type": "exec",
        "run_if": "passed",
        "on_cancel": null,
        "command": "bash",
        "arguments": [
          "-c",
          "./node_modules/.bin/nyc ./node_modules/.bin/mocha --opts mocha.opts"
