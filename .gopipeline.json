        "source": "container",
        "destination": null,
        "type": "build"
      }],
      "properties": [],
      "run_instance_count": null,
      "timeout": 0,
      "tasks": [{
        "type": "fetch",
        "run_if": "passed",
        "pipeline": "UsersClient",
        "stage": "Build",
        "job": "Build",
        "is_source_a_file": false,
        "source": "./node_modules",
        "destination": null
      }, {
        "type": "exec",
        "run_if": "passed",
        "on_cancel": null,
        "command": "npm",
        "arguments": [
          "rebuild"
        ],
        "working_directory": null
      }, {
        "type": "exec",
        "run_if": "passed",
        "on_cancel": null,
        "command": "./node_modules/.bin/webpack",
        "arguments": [],
        "working_directory": null
      }, {
        "type": "exec",
        "run_if": "passed",
        "on_cancel": null,
        "command": "mkdir",
        "arguments": [
          "dependencies"
        ],
        "working_directory": null
      }, {
        "type": "exec",
        "run_if": "passed",
        "on_cancel": null,
        "command": "curl",
        "arguments": [
          "-O",
          "https://raw.githubusercontent.com/CaseyCurry/LucaOpsEnvironment/master/dependencies/nodejs/latest/nodejs-6.9.5"
        ],
        "working_directory": "./dependencies"
      }, {
        "type": "exec",
        "run_if": "passed",
        "on_cancel": null,
        "command": "chmod",
        "arguments": [
          "u+x",
          "./nodejs-6.9.5"
        ],
        "working_directory": "dependencies"
      }, {
        "type": "exec",
        "run_if": "passed",
        "on_cancel": null,
        "command": "acbuild",
        "arguments": [
          "begin"
        ],
        "working_directory": null
      }, {
        "type": "exec",
        "run_if": "passed",
        "on_cancel": null,
        "command": "acbuild",
        "arguments": [
          "set-name",
          "luca.com/users-client"
        ],
        "working_directory": null
      }, {
        "type": "exec",
        "run_if": "passed",
        "on_cancel": null,
        "command": "/bin/bash",
        "arguments": [
          "-c",
          "acbuild label add version $GO_REVISION"
        ],
        "working_directory": null
      }, {
        "type": "exec",
        "run_if": "passed",
        "on_cancel": null,
        "command": "acbuild",
        "arguments": [
          "copy",
          "./dist",
          "/dist"
        ],
        "working_directory": null
      }, {
        "type": "exec",
        "run_if": "passed",
        "on_cancel": null,
        "command": "acbuild",
        "arguments": [
          "copy",
          "./dependencies/nodejs-6.9.5",
          "/nodejs-6.9.5"
        ],
        "working_directory": null
      }, {
        "type": "exec",
        "run_if": "passed",
        "on_cancel": null,
        "command": "acbuild",
        "arguments": [
          "set-exec",
          "/nodejs-6.9.5",
          "/dist/host.js"
        ],
        "working_directory": null
      }, {
        "type": "exec",
        "run_if": "passed",
        "on_cancel": null,
        "command": "acbuild",
        "arguments": [
          "port",
          "add",
          "http",
          "tcp",
          "8081"
        ],
        "working_directory": null
      }, {
        "type": "exec",
        "run_if": "passed",
        "on_cancel": null,
        "command": "bash",
        "arguments": [
          "-c",
          "mkdir ./container && acbuild write ./container/users-client.$GO_REVISION.aci"
        ],
        "working_directory": null
      }, {
        "type": "exec",
        "run_if": "passed",
        "on_cancel": null,
        "command": "acbuild",
        "arguments": [
          "end"
        ],
        "working_directory": null
